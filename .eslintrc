{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "import/no-unresolved": "error",
    "import/order": [
      "error",
      {
        "groups": [
          "external",
          "internal",
          "builtin",
          "sibling",
          "parent",
          "index"
        ]
      }
    ]
  }
}
